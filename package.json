    "gatsby-plugin-netlify-cms": "^6.0.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-sass": "^4.0.0",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-remark-emojis": "^0.5.0",
    "gatsby-remark-prismjs": "^5.0.0",
    "gatsby-source-filesystem": "^4.0.0",
