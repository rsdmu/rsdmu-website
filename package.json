{
  "name": "gatsby-starter-delog",
  "description": "A simplified blog for developers and designers built with Gatsby and Netlify CMS",
  "version": "1.6.0",
  "license": "MIT",
  "dependencies": {
    "gatsby": "^4.0.0",
    "gatsby-plugin-google-analytics": "^4.0.0",
    "gatsby-plugin-manifest": "^4.0.0",
